<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/list_product_nav"
    app:startDestination="@id/listProductFragment">

    <fragment
        android:id="@+id/listProductFragment"
        android:name="com.iagora.wingman.market.presentation.list_product.ListProductFragment"
        tools:layout="@layout/fragment_list_product_market"
        android:label="ListProductFragment">
        <argument
            android:name="idMarket"
            app:argType="string" />
        <action
            android:id="@+id/moveToAddProduct"
            app:destination="@id/add_product_nav"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim">
            <argument
                android:name="idMarket"
                app:argType="string" />
        </action>
    </fragment>
    <include app:graph="@navigation/add_product_nav" />
</navigation>